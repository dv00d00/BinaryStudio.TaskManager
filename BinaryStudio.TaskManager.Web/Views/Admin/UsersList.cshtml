@model IEnumerable<BinaryStudio.TaskManager.Logic.Domain.User>
@{ViewBag.Title = "Users List";}
<h1 class="page-header">
    Users List</h1>
<table>
    <tr>
        <th>
            UserName
        </th>
        <th>
            Email
        </th>
        <th>
            RoleId
        </th>
        <th>
            LinkedInId
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RoleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LinkedInId)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditUser", new { userId = item.Id }, new { @class = "btn" })
                @Html.ActionLink("Details", "DetailsUser", new { userId = item.Id }, new { @class = "btn" })
                @Html.ActionLink("Delete", "DeleteUser", new { userId = item.Id }, new { @class = "btn" })
            </td>
        </tr>
    }
</table>
<div>
    @Html.ActionLink(" Back to AdminPanel", "AdminPanel", "Admin", null, new { @class = "btn btn-primary" })
</div>
