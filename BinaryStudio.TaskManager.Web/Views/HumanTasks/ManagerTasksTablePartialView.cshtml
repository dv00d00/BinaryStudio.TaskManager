@model BinaryStudio.TaskManager.Logic.Domain.HumanTask
@* This partial view draw a task block in table *@        
<div class="task" id = @Model.Id.ToString() style="height: 100px">
    <a href = @Url.Action("Details",new {id = Model.Id})>
        <b>@Model.Name</b>
        <br/> <br/>
        @Model.Description
    </a> 
    
                                       
</div>
                           
                  
