@model BinaryStudio.TaskManager.Logic.Domain.HumanTask
@* This partial view draw a task block in table *@
@{ string divClass = "pr" + Model.Priority.ToString();
    
    <div class="task @divClass" id = @Model.Id.ToString()>
        <br />
        <b>@Model.Name</b>
        <br />
        @Model.Description
        <br />
        <div>@Html.ActionLink("Details", "Details", new { id = Model.Id }, new { @class = "label label-info" })</div>

    </div>
}
