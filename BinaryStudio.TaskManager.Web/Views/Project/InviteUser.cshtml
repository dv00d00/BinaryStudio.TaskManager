@model IEnumerable<BinaryStudio.TaskManager.Logic.Domain.User>
@{
    ViewBag.Title = "Список всех пользователей";
}
<h2>
    Список всех пользователей
</h2>
<table>
    <tr>
        <th>
            Имя пользователя
        </th>
        <th>
        </th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>
                @user.UserName
            </td>
            <td>
                @Html.ActionLink("Ivite", "AddUser", new { id = user.Id }, new { @class = "btn" }) 
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Back to tasks list", "PersonalProject")
</p>
